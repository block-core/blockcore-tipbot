{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "LOCAL": {
      "commandName": "Project",
      "commandLineArgs": "--local=appsettings.sample.json"
    },
    "X42": {
      "commandName": "Project",
      "commandLineArgs": "--chain=X42",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "CITY": {
      "commandName": "Project",
      "commandLineArgs": "--chain=CITY",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "XDS": {
      "commandName": "Project",
      "commandLineArgs": "--chain=XDS"
    },
    "Docker": {
      "commandName": "Docker",
      "commandLineArgs": "--chain=CITY",
      "publishAllPorts": true
    }
  }
}
